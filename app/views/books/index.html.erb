<main>
  <p id="notice"></p>
  <div class="container px-5 px-sm-0">
    <div class="row">
      <div class="col-md-3">
        <h2>User info</h2>
        <% if user.profile_image.attached? %>
         <%= image_tag user.profile_image,size: "100x100" %>
        <% else %>
         <%= image_tag 'no_image',size: "100x100" %>
        <% end %>
      </div>
    </div>
  </div>
</main>